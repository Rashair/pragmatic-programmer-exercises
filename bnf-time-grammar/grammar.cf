<time> ::=  <hour12> ":" <minute> <period_abbr> 
            | <hour24> ":" <minute>
            | <hour12> <period_abbr>
<hour12> ::= ("1" [0-2]) | ("0" <digit>)   | <digit>
<hour24> ::= ("2" [0-3]) | ([0-1] <digit>) | <digit> 
<minute> ::= ([0-5] <digit>)
<digit>  ::= [0-9]
<period_abbr> ::= "am" | "pm"