{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "curly": [
            "error",
            "multi",
            "consistent"
        ],
        "no-unused-vars": "warn",
        "prefer-template": "error"
    }
}